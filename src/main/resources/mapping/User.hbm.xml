<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yflog.entity.User" table="user" schema="plutus">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="native" />
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>